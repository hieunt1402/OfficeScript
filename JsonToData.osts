{"version":"0.3.0","body":"function main(workbook: ExcelScript.Workbook) {\n    // Sample JSON data (replace with your actual JSON)\n    const jsonData = `[\n  {\n    \"c1\": 1,\n    \"c2\": 2,\n    \"c3\": 3\n  },\n  {\n    \"c1\": 4,\n    \"c2\": 5,\n    \"c3\": 6\n  }\n]`;\n\n    // Parse the JSON data\n    let parsedData: { [key: string]: string }[] = JSON.parse(jsonData);\n\n    // Get the active worksheet\n    const sheet = workbook.getActiveWorksheet();\n\n    // Clear any existing content\n    let usedRg = sheet.getUsedRange()\n    if(usedRg){\n        usedRg.clear();\n    }\n\n    // Set headers\n    const headers = Object.keys(parsedData[0]);\n    sheet.getRange(\"A1\").getResizedRange(0, headers.length - 1).setValues([headers]);\n\n    // Populate the worksheet with the JSON data\n    const values = parsedData.map(item => headers.map(header => item[header]));\n    sheet.getRange(\"A2\").getResizedRange(values.length - 1, headers.length - 1).setValues(values);\n}","description":"","noCodeMetadata":"","parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}